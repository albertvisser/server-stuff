server {
    server_name local.magiokis.nl;
    root /home/albert/projects/magiokis/html;
    location /cgi-bin/ {
        gzip off;
        alias /home/albert/projects/magiokis/cgi-bin/;
        fastcgi_pass  unix:/var/run/fcgiwrap.socket;
        include /etc/nginx/fastcgi_params;
        fastcgi_param SCRIPT_FILENAME  /home/albert/projects/magiokis/cgi-bin$fastcgi_script_name;
        }
    error_log /var/log/nginx/local.magiokis-error.log error;
    access_log /var/log/nginx/local.magiokis-access.log ;
    }
server {
    server_name php.magiokis.nl;
    root /home/albert/projects/magiokis-php;
    #script_alias /home/albert/www/cgi-bin/magiokis/;
    error_log /var/log/nginx/php.magiokis-error.log error;
    access_log /var/log/nginx/php.magiokis-access.log ;
    location ~ \.php$ {
      # Filter out arbitrary code execution
      location ~ \..*/.*\.php$ {return 404;}

      include fastcgi_params;
      fastcgi_pass  unix:/tmp/php.socket;
      fastcgi_index index.php;
        }
    }
server {
    server_name data.magiokis.nl;
    root /home/albert/magiokis/data;
    #script_alias /home/albert/www/cgi-bin/magiokis/;
    error_log /var/log/nginx/data.magiokis-error.log error;
    access_log /var/log/nginx/data.magiokis-access.log ;
    }
server {
    server_name preview.magiokis.nl;
    root /home/albert/www/magiokis_vv/site;
    #script_alias /home/albert/www/cgi-bin/magiokis-vv/;
    error_log /var/log/nginx/preview.magiokis-error.log error;
    access_log /var/log/nginx/preview.magiokis-access.log ;
    }

