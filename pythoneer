server {
    server_name www.pythoneer.nl;
    root /home/albert/www/pythoneer;
    #script_alias /home/albert/www/cgi-bin/pythoneer/;
    location /cgi-bin/ {
        gzip off;
        alias /home/albert/www/cgi-bin/pythoneer/;
        fastcgi_pass  unix:/var/run/fcgiwrap.socket;
        include /etc/nginx/fastcgi_params;
        fastcgi_param SCRIPT_FILENAME  /home/albert/www/cgi-bin/pythoneer$fastcgi_script_name;
        }
    error_log /var/log/nginx/www.pythoneer-error.log error;
    access_log /var/log/nginx/www.pythoneer-access.log;
    }
